datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongoDb"]
}

model LinkContent {
  id  String  @id @default(auto()) @map("_id") @db.ObjectId
  email String @unique
  data Json 
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}

