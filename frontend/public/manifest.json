{
  "manifest_version": 3,
  "name": "save4me",
  "short_name": "save4me",
  "version": "1.0.0",
  "description": "Iâ€™ll help you to never lose your links again.",
  "icons": {
    "16": "plugin/16x16.png",
    "48": "plugin/48x48.png",
    "128": "plugin/128x128.png"
  },
  "oauth2": {
    "client_id": "132110520675-63bale6k5pfrq6jljbdv4emhatdg3dd3.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/userinfo.email"]
  },
  "key": "",
  "commands": {
    "save": {
      "suggested_key": {
        "default": "Ctrl+Shift+S",
        "mac": "Command+Shift+S"
      },
      "description": "Save the current page."
    },
    "_execute_action": {
      "suggested_key": {
        "windows": "Ctrl+Shift+S",
        "mac": "Command+Shift+S",
        "chromeos": "Ctrl+Shift+S",
        "linux": "Ctrl+Shift+S"
      }
    }
  },
  "permissions": ["activeTab", "scripting", "tabs", "storage", "clipboardRead", "identity"],
  "content_scripts": [
    {
      "matches": ["https://*/*", "http://*/*"],
      "js": ["inject.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "index.html"
  }
}
